<?xml version='1.0' encoding='utf-8'?>
<odoo noupdate="1">
    
    <record model="ir.sequence" id="course_booking_number_sequence" >
        <field name="name">Course Booking Number Sequence</field>
        <field name="code">course.booking.number.sequence</field>
        <field name="prefix">CB-</field>
        <field name="padding">5</field>
    </record>

    <record id="course_booking_cron" model="ir.cron">
        <field name="name">Cancel Course Booking</field>
        <field name="model_id" ref="model_course_booking"/>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="state">code</field>
        <field name="code">model._auto_cancel_course_booking()</field>
        <field name="priority">5</field>
        <field name="nextcall" eval="(datetime.now()).strftime('%Y-%m-%d 17:05:00')" />
    </record>

</odoo>